<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_main"
    app:startDestination="@id/loginFragment">

    <fragment
        android:id="@+id/loginFragment"
        android:name="kz.tutorial.jsonplaceholdertypicode.presentation.register.LoginFragment">
        <action
            android:id="@+id/action_loginFragment_to_registerFragment"
            app:destination="@id/registerFragment" />
        <action
            android:id="@+id/action_loginFragment_to_nav_second"
            app:destination="@id/nav_second" />
    </fragment>

    <fragment
        android:id="@+id/registerFragment"
        android:name="kz.tutorial.jsonplaceholdertypicode.presentation.register.RegisterFragment"/>

    <include app:graph="@navigation/nav_second"/>
</navigation>